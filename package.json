{
	"name": "monorepo",
	"private": "true",
	"author": "Eyas Sharaiha <eyas@google.com> (https://eyas.sh/), Warren Halderman <halderman.warren@gmail.com>",
	"maintainers": [
		"Warren Halderman <halderman.warren@gmail.com>"
	],
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@jest/globals": "^29.4.3",
		"@types/node": "^22.9.1",
		"jest": "^29.4.3",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.0.5",
		"typescript": "^5.6.3"
	},
	"engines": {
		"node": ">=14.0.0",
		"pnpm": ">=9.0.0"
	},
	"engineStrict": true,
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts",
			"dist/**/*",
			"coverage/**/*",
			"test/**/*",
			"src/cli/cli.ts"
		],
		"reporter": [
			"html"
		],
		"all": true
	},
	"homepage": "https://github.com/whalderman/schema-dts",
	"bugs": "https://github.com/whalderman/schema-dts/issues",
	"repository": "github:whalderman/schema-dts",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf \"packages/**/dist\" \"packages/**/*.tsbuildinfo\" packages/schema-dts/lib/*",
		"build": "pnpm recursive run build",
		"lint": "biome check",
		"fix": "biome check --write",
		"test": "pnpm recursive run lint && pnpm recursive run test",
		"coverage_on_travis": "pnpm run coverage_on_travis -w schema-dts-gen",
		"pkg": "pnpm run clean && pnpm run build"
	},
	"type": "module",
	"packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a",
	"dependencies": {
		"@types/jest": "^29.5.14",
		"jest-mock": "^29.7.0"
	}
}
