{
	"name": "schema-dts",
	"version": "1.1.3",
	"displayName": "schema-dts: Strongly-typed Schema.org vocabulary declarations",
	"description": "A TypeScript package with latest Schema.org Schema Typings",
	"author": "Eyas Sharaiha <eyas@google.com> (https://eyas.sh/), Warren Halderman <halderman.warren@gmail.com>",
	"maintainers": ["Warren Halderman <halderman.warren@gmail.com>"],
	"contributors": [
		"Eyas Sharaiha <npm@eyas.sh> (https://eyas.sh/)",
		"Warren Halderman <halderman.warren@gmail.com>"
	],
	"files": ["dist/schema.d.ts", "dist/schema.js", "LICENSE", "README.md"],
	"types": "./dist/schema.d.ts",
	"main": "./dist/schema.js",
	"devDependencies": {
		"mkdirp": "^1.0.4",
		"schema-dts-gen": "^1.1.2"
	},
	"keywords": [
		"typescript",
		"tsd",
		"dts",
		"schema.org",
		"Semantic Web",
		"semantic-web",
		"Linked Data",
		"linked-data",
		"jsonld",
		"JSON-LD",
		"structured data",
		"structured-data"
	],
	"bugs": "https://github.com/whalderman/schema-dts/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/whalderman/schema-dts.git",
		"directory": "packages/schema-dts"
	},
	"scripts": {
		"lint:biome": "biome check test/**/*.ts",
		"prepare": "mkdirp lib/",
		"build:generate": "schema-dts-gen > lib/schema.ts",
		"build:compile": "tsc -p .",
		"build": "pnpm run build:generate && pnpm run build:compile",
		"test": "pnpm run build && tsc -p test/"
	},
	"license": "Apache-2.0"
}
